<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<navigation-rule>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>accepted</from-outcome>
			<to-view-id>/main.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>rejected</from-outcome>
			<to-view-id>/erreur.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/destinations.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>updateDestination</from-outcome>
			<to-view-id>/updateDestination.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/hotels.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>updateHotel</from-outcome>
			<to-view-id>/updateHotel.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/travelers.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>updateTraveler</from-outcome>
			<to-view-id>/updateTraveler.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/bookings.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>updateBooking</from-outcome>
			<to-view-id>/updateBooking.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/comments.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>updateComment</from-outcome>
			<to-view-id>/updateComment.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
